class Geeks {

    static void isPrime(int n) {

        if(n==1){

            System.out.println("No");

            return;

        }

        int count=0;

        for (int i = 2; i <= Math.sqrt(n); i++) {

            // Your code here

            if(n%i==0){

             count++;

            }

        }

        if(count>=1){

            System.out.println("No");

        }else{

            System.out.println("Yes");

        }

    }

}
